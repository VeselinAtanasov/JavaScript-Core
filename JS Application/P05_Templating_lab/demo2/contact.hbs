<article>
    {{#if title}}
        <h2>{{title}}</h2>
    {{else}}
        <h2>No Title</h2>
    {{/if}}

    {{#each contacts}}
        <div class="title">
            {{name}}
            <button>&#8505;</button>
        </div>
        <div class="info">
            <span>&phone; {{phone}}</span>
            <span>&#9993; {{email}}</span>
        </div>
        {{> partialM}}
    {{else}}
        <div class="title">
            No Data
        </div>
    {{/each}}
</article>
